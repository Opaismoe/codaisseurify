<%= form_for [@artist, @artist.songs.new] do |f| %>
  <div class="field">

    <% @artist.errors.full_messages.each do |msg| %>
      <li class="alert alert-danger"><%= msg %></li>
    <% end %>

    <%= f.text_field :name, placeholder: "Name of the song you'd like to add", style: "width: 90%; height: 35px; background-color: rgb(42, 42, 42)"; %>
    <%= f.submit "Add", class: "btn btn-success" %>

  </div>
<% end %>
